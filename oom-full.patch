+++ b/lib/db_supabase/resultsRepo.ts
@@ -0,0 +1,56 @@
+import { supabase } from "@/lib/supabase";
+
+export type EventResultDoc = {
+  id: string;
+  event_id: string;
+  member_id: string;
+  points: number;
+  created_at?: string;
+  updated_at?: string;
+};
+
+export async function getEventResults(eventId: string): Promise<EventResultDoc[]> {
+  const { data, error } = await supabase
+    .from("event_results")
+    .select("member_id,points")
+    .eq("event_id", eventId);
+
+  if (error) {
+    console.error("[resultsRepo] getEventResults failed:", {
+      message: error.message,
+      details: error.details,
+      hint: error.hint,
+      code: error.code,
+    });
+    return [];
+  }
+
+  return (data ?? []).map((row) => ({
+    id: "",
+    event_id: eventId,
+    member_id: row.member_id,
+    points: row.points,
+  }));
+}
+
+export async function upsertEventResults(
+  eventId: string,
+  rows: { member_id: string; points: number }[]
+): Promise<void> {
+  if (!rows.length) return;
+
+  const payload = rows.map((row) => ({
+    event_id: eventId,
+    member_id: row.member_id,
+    points: row.points,
+  }));
+
+  const { error } = await supabase
+    .from("event_results")
+    .upsert(payload, { onConflict: "event_id,member_id" });
+
+  if (error) {
+    console.error("[resultsRepo] upsertEventResults failed:", {
+      message: error.message,
+      details: error.details,
+      hint: error.hint,
+      code: error.code,
+    });
+    throw new Error(error.message || "Failed to save event results");
+  }
+}
